{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:1420"
        },
        {
          "url": "https://pivert.falcolabs.org"
        }
      ],
      "deny": []
    },
    "store:default"
  ]
}